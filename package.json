{
  "name": "@sparing-software/eslint-config",
  "version": "1.3.8",
  "description": "",
  "scripts": {
    "prepare": "is-ci || husky install",
    "pack-windows": "npm pack && powershell -Command \"Clear-Host; $tgzFile = Get-ChildItem -Filter *.tgz | Select-Object -First 1; Write-Host \\\"$(Get-Location)\\$($tgzFile.Name)\\\"\"",
    "pack-macos": "npm pack && clear && echo \"$(pwd)/$(ls -d *.tgz | head -n 1)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SparingSoftware/eslint-config.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "overrides": {
    "@react-native/eslint-config": {
      "eslint": "$eslint"
    },
    "eslint-plugin-react-native-a11y": {
      "eslint": "$eslint"
    }
  },
  "author": "Sparing Software",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SparingSoftware/eslint-config/issues"
  },
  "homepage": "https://github.com/SparingSoftware/eslint-config#readme",
  "engines": {
    "node": ">=18.18.0 || >=20.9.0 || >=21.0.0"
  },
  "exports": {
    ".": "./configs/main.mjs",
    "./react": "./configs/react.mjs",
    "./astro": "./configs/astro.mjs"
  },
  "peerDependencies": {
    "eslint": ">= 9"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^14.2.15",
    "@nuxt/eslint-config": "^0.6.0",
    "@react-native/eslint-config": "^0.75.4",
    "@tanstack/eslint-plugin-query": "^5.59.7",
    "@types/eslint": "^9.6.1",
    "@vue/eslint-config-typescript": "^14.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-astro": "^1.3.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-native-a11y": "^3.5.0",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-vue": "^9.29.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "globals": "^15.11.0",
    "typescript-eslint": "^8.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@eslint/compat": "^1.2.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@semantic-release/git": "^10.0.1",
    "@sparing-software/commitlint-config": "^1.0.1",
    "@sparing-software/prettier-config": "^1.0.0",
    "@types/eslint__js": "^8.42.3",
    "husky": "^9.1.6",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "semantic-release": "^24.1.2"
  }
}
